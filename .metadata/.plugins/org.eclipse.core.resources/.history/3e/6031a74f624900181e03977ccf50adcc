package pages;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;
import org.openqa.selenium.support.How;

public class EditTopic {

	private static WebDriver existingTopicName;

	public EditTopic(WebDriver driver) {
		this.existingTopicName = driver;
	}
 
	 @FindBy(how=How.XPATH,using="")
	 private WebElement topicThreeDot;
	 
	 
	 public void editTopicName()
	 {
		driver.findElementexistingTopicName+"/parent::*/following-sibling::div//i";
		 
		 
	 }
}
